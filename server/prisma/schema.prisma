datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model HzWaterMeter {
  createdAt      DateTime  @default(now())
  id             String    @id @default(cuid())
  installAddress String?
  installDate    DateTime?
  isTotal        Boolean?
  lastUpdateTime DateTime?
  lat            Float?
  lon            Float?
  manufacture    String?
  meterId        String?
  meterName      String?
  notes          String?
  pressure       Float?
  totalValue     Float?
  updatedAt      DateTime  @updatedAt
}